class Solution {
    public boolean isAnagram(String s, String t) {
  if(s.length != t.length){
  return false;
}
List<Character> a = new ArrayList();
List<Character> b = new ArrayList();
for(char c:s.toCharArray()){
a.add(c);
}
for(char c:t.toCharArray()){
b.add(c);
}

Collections.sort(a);
Collections.sort(b);
if(a.equals(b)){
return true;
}else{
return false;}
}

